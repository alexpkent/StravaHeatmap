{
  "bindings": [
    {
      "authLevel": "anonymous",
      "connection": "BLOB_CONNECTION_STRING",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "activities"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "stravaBlobIn",
      "type": "blob",
      "path": "strava/strava.json",
      "connection": "BLOB_CONNECTION_STRING",
      "direction": "in"
    },
    {
      "name": "stravaBlobOut",
      "type": "blob",
      "path": "strava/strava.json",
      "connection": "BLOB_CONNECTION_STRING",
      "direction": "out"
    }
  ]
}